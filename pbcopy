package main

import (
	"log"
	"net"

	"google.golang.org/grpc"

	"github.com/tetsuzawa/go-grpc-test/proto/cat"
	"github.com/tetsuzawa/go-grpc-test/proto/ping"
	"github.com/tetsuzawa/go-grpc-test/service"
)

func main() {
	listenPort, err := net.Listen("tcp", ":19003")
	if err != nil {
		log.Fatalln(err)
	}
	server := grpc.NewServer()
	catService := &service.MyCatService{}
	pingService := &service.PingService{}
	// 実行したい実処理をseverに登録する
	cat.RegisterCatServer(server, catService)
	ping.RegisterPingServer(server, pingService)
	err = server.Serve(listenPort)
	if err != nil {
		log.Fatalln(err)
	}
}
